        <result column="end" property="end" />
        <result column="startTime" property="startTime" />
        <result column="price" property="price" />
        <result column="mileage" property="mileage" />
        <result column="linesId" property="linesId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, searchValue, createBy, createTime, updateBy, updateTime, remark, endTime, name, first, end, startTime, price, mileage, linesId
