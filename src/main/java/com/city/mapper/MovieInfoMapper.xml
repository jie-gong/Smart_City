<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.city.mapper.MovieInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.city.bean.MovieInfo">
        <id column="id" property="id" />
        <result column="Movieid" property="Movieid" />
        <result column="MovieName" property="MovieName" />
        <result column="Moviecover" property="Moviecover" />
        <result column="showDate" property="showDate" />
        <result column="timeLength" property="timeLength" />
        <result column="Moviemark" property="Moviemark" />
        <result column="markNum" property="markNum" />
        <result column="wantedNum" property="wantedNum" />
        <result column="lookedNum" property="lookedNum" />
        <result column="movieDesc" property="movieDesc" />
        <result column="thumbup" property="thumbup" />
        <result column="MovieType" property="MovieType" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, Movieid, MovieName, Moviecover, showDate, timeLength, Moviemark, markNum, wantedNum, lookedNum, movieDesc, thumbup, MovieType
    </sql>

</mapper>
