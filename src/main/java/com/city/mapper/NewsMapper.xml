    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, searchValue, createBy, createTime, updateBy, updateTime, dictCode, title, content, imgUrl, pressCategory, isRecommend, likeNumber, viewsNumber, userId, pressStatus, pressId, theme
    </sql>

</mapper>
