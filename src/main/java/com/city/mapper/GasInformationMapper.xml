<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.city.mapper.GasInformationMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.city.bean.GasInformation">
        <id column="id" property="id" />
        <result column="gasname" property="gasname" />
        <result column="img" property="img" />
        <result column="release_time" property="releaseTime" />
        <result column="read" property="read" />
        <result column="msg" property="msg" />
        <result column="like" property="like" />
        <result column="gasid" property="gasid" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, gasname, img, release_time, read, msg, like, gasid
    </sql>

</mapper>
