<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.city.mapper.MovieStatusMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.city.bean.MovieStatus">
        <id column="id" property="id" />
        <result column="movieid" property="movieid" />
        <result column="status" property="status" />
        <result column="username" property="username" />
        <result column="statusmsg" property="statusmsg" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, movieid, status, username, statusmsg
    </sql>

</mapper>
