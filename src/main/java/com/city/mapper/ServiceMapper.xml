<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.city.mapper.ServiceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.city.bean.Service">
        <id column="id" property="id" />
        <result column="searchValue" property="searchValue" />
        <result column="createBy" property="createBy" />
        <result column="createTime" property="createTime" />
        <result column="updateBy" property="updateBy" />
        <result column="updateTime" property="updateTime" />
        <result column="remark" property="remark" />
        <result column="serviceName" property="serviceName" />
        <result column="serviceDesc" property="serviceDesc" />
        <result column="serviceType" property="serviceType" />
        <result column="imgUrl" property="imgUrl" />
        <result column="pid" property="pid" />
        <result column="isRecommend" property="isRecommend" />
        <result column="link" property="link" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        searchValue, createBy, createTime, updateBy, updateTime, remark, id, serviceName, serviceDesc, serviceType, imgUrl, pid, isRecommend, link
    </sql>

</mapper>
